{
  "name": "incident-reporting-system",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/server/index.js",
    "preinstall": "npm cache clean --force",
    "dev": "concurrently \"npm run server\" \"npm run startclient\"",
    "startclient": "vite",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci --coverage",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "server": "nodemon --watch \"server/**/*.ts\" --exec \"tsx\" server/index.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "incident-reporting-system": "file:",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-datepicker": "^7.4.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.2.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^3.1.0",
    "bootstrap": "^5.2.3",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^4.1.0",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
